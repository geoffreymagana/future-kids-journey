{
  "info": {
    "name": "Future Kids Journey API",
    "description": "Supabase Hybrid Integration Testing",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{auth_token}}",
        "type": "string"
      }
    ]
  },
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@example.com\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/auth/login",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "auth",
                "login"
              ]
            }
          }
        },
        {
          "name": "Get Current Admin",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/auth/me",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "auth",
                "me"
              ]
            }
          }
        },
        {
          "name": "List All Admins",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/auth/admins",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "auth",
                "admins"
              ]
            }
          }
        },
        {
          "name": "Create Admin",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"newadmin@example.com\",\n  \"password\": \"password123\",\n  \"role\": \"admin\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/auth/register",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "auth",
                "register"
              ]
            }
          }
        },
        {
          "name": "Logout",
          "request": {
            "method": "POST",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/auth/logout",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "auth",
                "logout"
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Forms",
      "item": [
        {
          "name": "Submit Form",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"John Doe\",\n  \"whatsapp\": \"1234567890\",\n  \"ageRange\": \"5-7\",\n  \"numberOfKids\": 2,\n  \"source\": \"facebook\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/forms/submit",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "forms",
                "submit"
              ]
            }
          }
        },
        {
          "name": "List Submissions",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/forms/submissions?page=1&limit=50",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "forms",
                "submissions"
              ],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "50"
                },
                {
                  "key": "status",
                  "value": "new",
                  "disabled": true
                }
              ]
            }
          }
        },
        {
          "name": "Get Submission",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/forms/submissions/:id",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "forms",
                "submissions",
                ":id"
              ],
              "variable": [
                {
                  "key": "id",
                  "value": ""
                }
              ]
            }
          }
        },
        {
          "name": "Update Submission Status",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"status\": \"contacted\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/forms/submissions/:id",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "forms",
                "submissions",
                ":id"
              ],
              "variable": [
                {
                  "key": "id",
                  "value": ""
                }
              ]
            }
          }
        },
        {
          "name": "Get Dashboard Stats",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/forms/stats",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "forms",
                "stats"
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Enrollments",
      "item": [
        {
          "name": "List Enrollments",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/enrollments",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "enrollments"
              ]
            }
          }
        },
        {
          "name": "Create Enrollment",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"submission_id\": \"uuid-here\",\n  \"enrollment_amount\": 5000\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/enrollments",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "enrollments"
              ]
            }
          }
        },
        {
          "name": "Get Enrollment",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/enrollments/:id",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "enrollments",
                ":id"
              ],
              "variable": [
                {
                  "key": "id",
                  "value": ""
                }
              ]
            }
          }
        },
        {
          "name": "Update Enrollment",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"status\": \"completed\",\n  \"enrollment_amount\": 5500\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/enrollments/:id",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "enrollments",
                ":id"
              ],
              "variable": [
                {
                  "key": "id",
                  "value": ""
                }
              ]
            }
          }
        },
        {
          "name": "Delete Enrollment",
          "request": {
            "method": "DELETE",
            "header": [],
            "url": {
              "raw": "{{base_url}}/api/enrollments/:id",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "api",
                "enrollments",
                ":id"
              ],
              "variable": [
                {
                  "key": "id",
                  "value": ""
                }
              ]
            }
          }
        }
      ]
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:3000",
      "type": "string"
    },
    {
      "key": "auth_token",
      "value": "",
      "type": "string"
    }
  ]
}
